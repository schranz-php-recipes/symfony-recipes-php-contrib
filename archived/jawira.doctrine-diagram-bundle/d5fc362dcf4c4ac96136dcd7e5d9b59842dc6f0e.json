                "bundles": {
                    "Jawira\\DoctrineDiagramBundle\\DoctrineDiagramBundle": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                }
            },
            "files": {
