{
    "manifests": {
        "sensiolabs/minify-bundle": {
            "manifest": {
                "bundles": {
                    "Sensiolabs\\MinifyBundle\\SensiolabsMinifyBundle": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                },
                "gitignore": [
                    "/%VAR_DIR%/minify/"
                ]
            },
            "files": {
                "config/packages/sensiolabs_minify.php": {
                    "contents": [
                        "<?php",
                        "",
                        "declare(strict_types=1);",
                        "",
                        "use Symfony\\Component\\DependencyInjection\\Loader\\Configurator\\ContainerConfigurator;",
                        "",
                        "return static function (ContainerConfigurator $containerConfigurator): void {",
                        "    $containerConfigurator->extension('sensiolabs_minify', [",
                        "        'asset_mapper' => [",
                        "            'ignore_paths' => [",
                        "                '*.min.js',",
                        "                '*.min.css',",
                        "            ],",
                        "        ],",
                        "        'minify' => [",
                        "            'download_binary' => true,",
                        "        ],",
                        "    ]);",
                        "    if ($containerConfigurator->env() === 'test') {",
                        "        $containerConfigurator->extension('sensiolabs_minify', [",
                        "            'asset_mapper' => false,",
                        "        ]);",
                        "    }",
                        "};",
                        ""
                    ],
                    "executable": false
                }
            },
            "ref": "bf77536a11690f347828dceed68d02bfc986c9fa"
        }
    }
}
