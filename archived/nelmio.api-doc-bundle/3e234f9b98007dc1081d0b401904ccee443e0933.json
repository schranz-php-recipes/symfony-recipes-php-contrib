{
    "manifests": {
        "nelmio/api-doc-bundle": {
            "manifest": {
                "bundles": {
                    "Nelmio\\ApiDocBundle\\NelmioApiDocBundle": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                }
            },
            "files": {
                "config/packages/nelmio_api_doc.php": {
                    "contents": [
                        "<?php",
                        "",
                        "declare(strict_types=1);",
                        "",
                        "use Symfony\\Component\\DependencyInjection\\Loader\\Configurator\\ContainerConfigurator;",
                        "",
                        "return static function (ContainerConfigurator $containerConfigurator): void {",
                        "    $containerConfigurator->extension('nelmio_api_doc', [",
                        "        'documentation' => [",
                        "            'info' => [",
                        "                'title' => 'My App',",
                        "                'description' => 'This is an awesome app!',",
                        "                'version' => '1.0.0',",
                        "            ],",
                        "        ],",
                        "        'areas' => [",
                        "            'path_patterns' => [",
                        "                '^/api(?!/doc$)',",
                        "            ],",
                        "        ],",
                        "    ]);",
                        "};",
                        ""
                    ],
                    "executable": false
                },
                "config/routes/nelmio_api_doc.php": {
                    "contents": [
                        "<?php",
                        "",
                        "declare(strict_types=1);",
                        "",
                        "use Symfony\\Component\\Routing\\Loader\\Configurator\\RoutingConfigurator;",
                        "",
                        "return static function (RoutingConfigurator $routingConfigurator): void {",
                        "    $routingConfigurator->add('app.swagger', '/api/doc.json')",
                        "        ->controller('nelmio_api_doc.controller.swagger')",
                        "        ->methods([",
                        "            'GET',",
                        "        ]);",
                        "};",
                        ""
                    ],
                    "executable": false
                }
            },
            "ref": "3e234f9b98007dc1081d0b401904ccee443e0933"
        }
    }
}
