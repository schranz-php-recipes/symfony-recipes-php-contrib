{
    "manifests": {
        "datana-gmbh/fake-api-client": {
            "manifest": {
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                },
                "env": {
                    "FAKE_API_BASE_URI": "",
                    "FAKE_API_USERNAME": "",
                    "FAKE_API_PASSWORD": "",
                    "FAKE_API_DISABLE_CACHE": "false"
                }
            },
            "files": {
                "config/packages/datana_fake_api.php": {
                    "contents": [
                        "<?php",
                        "",
                        "declare(strict_types=1);",
                        "",
                        "use Datana\\FakeApi\\Formulario\\DateneingabenApi;",
                        "use Datana\\FakeApi\\Formulario\\StatisticsApi;",
                        "use Symfony\\Component\\DependencyInjection\\Loader\\Configurator\\ContainerConfigurator;",
                        "",
                        "return static function (ContainerConfigurator $containerConfigurator): void {",
                        "    $services = $containerConfigurator->services();",
                        "",
                        "    $services->defaults()",
                        "        ->autowire();",
                        "",
                        "    $services->set('Datana\\FakeApi\\FakeApiClient')",
                        "        ->args([",
                        "            '%env(FAKE_API_BASE_URI)%',",
                        "            '%env(FAKE_API_USERNAME)%',",
                        "            '%env(FAKE_API_PASSWORD)%',",
                        "            '%env(bool:FAKE_API_DISABLE_CACHE)%',",
                        "        ]);",
                        "",
                        "    $services->set(DateneingabenApi::class);",
                        "",
                        "    $services->set(StatisticsApi::class);",
                        "};",
                        ""
                    ],
                    "executable": false
                }
            },
            "ref": "ba1a5e3458ec16973afb313ba8937bc6746e13a8"
        }
    }
}
