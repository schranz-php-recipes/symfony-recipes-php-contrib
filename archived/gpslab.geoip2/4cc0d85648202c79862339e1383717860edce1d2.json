{
    "manifests": {
        "gpslab/geoip2": {
            "manifest": {
                "bundles": {
                    "GpsLab\\Bundle\\GeoIP2Bundle\\GpsLabGeoIP2Bundle": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                },
                "composer-scripts": {
                    "geoip2:update": "symfony-cmd"
                }
            },
            "files": {
                "config/packages/gpslab_geoip.php": {
                    "contents": [
                        "<?php",
                        "",
                        "declare(strict_types=1);",
                        "",
                        "use Symfony\\Component\\DependencyInjection\\Loader\\Configurator\\ContainerConfigurator;",
                        "",
                        "return static function (ContainerConfigurator $containerConfigurator): void {",
                        "    $containerConfigurator->extension('gpslab_geoip', [",
                        "        'cache' => '%kernel.cache_dir%/GeoLite2-City.mmdb',",
                        "        'url' => 'https://geolite.maxmind.com/download/geoip/database/GeoLite2-City.mmdb.gz',",
                        "        'locales' => [",
                        "            'en',",
                        "        ],",
                        "    ]);",
                        "};",
                        ""
                    ],
                    "executable": false
                }
            },
            "ref": "4cc0d85648202c79862339e1383717860edce1d2"
        }
    }
}
