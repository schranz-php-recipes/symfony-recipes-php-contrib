{
    "manifests": {
        "mezcalito/ux-search": {
            "manifest": {
                "bundles": {
                    "Mezcalito\\UxSearchBundle\\MezcalitoUxSearchBundle": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                },
                "env": {
                    "#1": "MEZCALITO_UX_SEARCH_DEFAULT_DSN=\"meilisearch://key@localhost:7700\"",
