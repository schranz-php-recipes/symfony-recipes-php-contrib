{
    "bundles": {
        "EcPhp\\EuLoginBundle\\EuLoginBundle": [
            "all"
        ]
    },
    "copy-from-recipe": {
        "config/": "%CONFIG_DIR%/"
    },
    "env": {
        "ECAS_BASE_URL": "https://ecas.ec.europa.eu/cas",
        "ECAS_SERVICE": "",
        "ECAS_LOGOUT_SERVICE_URL": "",
        "ECAS_PGT_CALLBACK_URL": "",
        "ECAS_PROXY_ASSURANCE_LEVEL": "",
