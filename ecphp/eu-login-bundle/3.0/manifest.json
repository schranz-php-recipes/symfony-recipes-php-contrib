        "ECAS_ASSURANCE_LEVEL_MEDIUM": "/sponsorValidate",
        "ECAS_ASSURANCE_LEVEL_HIGH": "/interinstitutionalValidate",
        "ECAS_ASSURANCE_LEVEL_TOP": "/strictValidate",
        "ECAS_ASSURANCE_LEVEL": "${ECAS_ASSURANCE_LEVEL_TOP}"
