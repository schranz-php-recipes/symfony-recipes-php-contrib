                        "    data: tests/Support/Data",
                        "    support: tests/Support",
                        "    envs: tests/_envs",
                        "actor_suffix: Tester",
                        "extensions:",
                        "    enabled:",
                        "        - Codeception\\Extension\\RunFailed",
                        "params:",
                        "    - .env",
                        "    - .env.test",
                        "settings:",
